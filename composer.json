{
    "name": "medicalmundi/npi-registry-php-usdk",
    "description": "HTTP client for the NPI Registry Public records api.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zerai Teclai",
            "email": "teclaizerai@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "juststeveking/php-sdk": "^1.1",
        "nyholm/psr7": "^1.0",
        "php-di/php-di": "^6.0",
        "symfony/http-client": "^4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MedicalMundi\\NpiRegistry\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MedicalMundi\\NpiRegistry\\SDK\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    }
}
